<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BlockchainTitleDeeds" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue500</item>
        <item name="colorPrimaryDark">@color/blue800</item>
        <item name="colorSecondary">@color/yellow700</item>
        <item name="android:statusBarColor">@color/black30</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.BlockchainTitleDeeds.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.BlockchainTitleDeeds.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.BlockchainTitleDeeds.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>